<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>ChaseLabs.ChaseLabs.CLLogger</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageId>ChaseLabs.Logger</PackageId>
    <RepositoryUrl>https://github.com/DcmanProductions/CLLogger.git</RepositoryUrl>
    <PackageProjectUrl>https://github.com/DcmanProductions/CLLogger</PackageProjectUrl>
    <RepositoryType>GitHub</RepositoryType>
    <Version>0.0.7</Version>
    <Authors>Drew Chase</Authors>
    <Company>ChaseLabs</Company>
    <Product>Simple Logging</Product>
    <Description>An easy to use file logging system.</Description>
    <Copyright>2020</Copyright>
    <PackageReleaseNotes>Fixed Ambiguous usage of Path and SetPath</PackageReleaseNotes>
    <PackageTags>Utilities Logging Log ChaseLabs Helpers</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>D:\Workspaces\Visual Studio Workspace\C#\Class Library\Libraries\CLLogger\CLLogger.xml</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Remove=".gitignore" />
    <None Remove="LICENSE" />
  </ItemGroup>

</Project>
